class Stars
{
  int flag;
  String displayName;
  int distance;
  float x;
  float y;
  float z;
  long starSize;
  
  Stars(int flag,String displayName,int distance,float x, float y, float z, long starSize)
  {
    this.flag = flag;
    this.displayName = displayName;
    this.distance = distance;
    this.x = x;
    this.y = y;
    this.z = z;
    this.starSize = starSize;
  }
  
  
}